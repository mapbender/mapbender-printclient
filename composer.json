{
    "name": "mapbender/print",
    "description": "Mapbender print service element",
    "keywords": ["mapbender", "print"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "Stefan Winkelmann"}
    ],
    "require": {
        "php": ">=5.3.3",

        "arsgeografica/signing": "1.1.x",
        "setasign/fpdi-fpdf": "1.6.*",

        "mapbender/owsproxy": "dev-release/3.0.6 as 1.2.0",

        "symfony/framework-bundle" :"2.x"
    },
    "require-dev": {
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
		"psr-0": {"Mapbender\\PrintBundle": "."}
    },
    "target-dir": "Mapbender/PrintBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-release/1.0": "1.0.x-dev"
        }
    }
}
