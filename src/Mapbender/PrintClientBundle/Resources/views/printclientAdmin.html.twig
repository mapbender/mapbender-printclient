<div class="elementFormPrintClient elementForm">
    <div class="left">
        {{ form_widget(form.configuration.autoOpen) }}<label for="{{form.configuration.autoOpen.vars.id}}" class="labelCheck">{{"mb.core.admin.printclient.label.autoopen"|trans}}</label>
    </div>
    <div class="clearContainer"></div>
    {{ form_label(form.title) }}{{ form_widget(form.title) }}
    <div class="clearContainer"></div>
    {{ form_label(form.configuration.target) }}{{ form_widget(form.configuration.target) }}
    <div class="clearContainer"></div>
    {{ form_label(form.configuration.scales) }}{{ form_widget(form.configuration.scales) }}
    <div class="clearContainer"></div>
    {{ form_widget(form.configuration.rotatable) }}<label for="{{form.configuration.rotatable.vars.id}}" class="labelCheck">{{"mb.core.admin.printclient.label.rotatable"|trans}}</label>
    <div class="clearContainer"></div>
    {{ form_widget(form.configuration.legend) }}<label for="{{form.configuration.legend.vars.id}}" class="labelCheck">{{"mb.core.admin.printclient.label.legend"|trans}}</label>
    <div class="clearContainer"></div>
    {{ form_label(form.configuration.file_prefix) }}{{ form_widget(form.configuration.file_prefix) }}
    <div class="clearContainer"></div>
    {{ form_label(form.configuration.quality_levels) }}
    {% for level in form.configuration.quality_levels %}
        <div class="singleRow">
            <div class="left">
                {{form_label(level.dpi)}}
                {{ form_widget(level.dpi) }}
            </div>
            <div class="left">
                {{form_label(level.label)}}
                {{ form_widget(level.label) }}
            </div>
        </div>
        <div class="clearContainer"></div>
        <span class="labelInput"></span>
    {% endfor %}
    <div class="clearContainer"></div>
    {% for template in form.configuration.templates %}
        {{form_label(template.template)}}   {{ form_widget(template.template) }}
        <div class="clearContainer"></div>
        {{form_label(template.label)}}   {{ form_widget(template.label) }}
        <div class="clearContainer"></div>
        {{form_label(template.format)}}   {{ form_widget(template.format) }}
        <div class="clearContainer"></div>
    {% endfor %}
	{{ form_label(form.configuration.optional_fields) }}{{ form_widget(form.configuration.optional_fields) }}
    {{ form_label(form.configuration.replace_pattern) }}{{ form_widget(form.configuration.replace_pattern) }}
</div>
